<?xml version="1.0" ?>
<odoo>

    <record id="view_config_settings_with_attachment_from_google_drive" model="ir.ui.view">
        <field name="name">Config Settings With Attachment From Google Drive</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="16"/>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div name="integration" position="inside">
                <div class="col-xs-12 col-md-6 o_setting_box">
                    <div class="o_setting_right_pane">
                        <div class="mt16 row">
                            <label for="google_attachment_client_id" class="col-md-3 o_light_label"/>
                            <field name="google_attachment_client_id"/>
                        </div>
                        <div class="mt16 row">
                            <label for="google_attachment_api_key" class="col-md-3 o_light_label"/>
                            <field name="google_attachment_api_key"/>
                        </div>
                    </div>
                </div>
            </div>
        </field>
    </record>

</odoo>
