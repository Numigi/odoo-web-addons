<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-inherit="mail.AttachmentBox" t-inherit-mode="extension">
        <xpath expr="//*[hasclass('o_AttachmentBox_buttonAdd')]" position="after">
            <GoogleAttachment
                uploadModel="thread.model"
                uploadId="thread.id"
            	/>
        </xpath>
    </t>

</templates>
